{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Stores",
  "type": "object",
  "properties": {
    "city": {
      "type": "string"
    },
    "company": {
      "type": "string"
    },
    "stores": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "schedules": {
            "type": "string"
          },
          "address": {
            "type": "string"
          },
          "latitude": {
            "type": "string"
          },
          "longitude": {
            "type": "string"
          },
          "status": {
            "type": "boolean"
          }
        },
        "required": [
          "id",
          "name",
          "schedules",
          "address",
          "latitude",
          "longitude",
          "status"
        ]
      }
    }
  },
  "required": ["city", "stores"]
}
