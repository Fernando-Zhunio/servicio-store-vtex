{
  "name": "service-example",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,graphql,gql}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/node": "18.11.9",
    "@vtex/danger": "0.2.9",
    "@vtex/prettier-config": "0.3.6",
    "@vtex/tsconfig": "0.6.0",
    "eslint": "7.32.0",
    "eslint-config-vtex": "15.0.2",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "typescript": "4.9.3"
  },
  "dependencies": {
    "axios": "1.2.0",
    "co-body": "6.1.0"
  }
}
